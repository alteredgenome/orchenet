# OrcheNet Agent Configuration Example
# Copy this file to agent.yaml and configure for your device

# Server connection
server_url: "https://orchenet.example.com"

# Device identification
device_id: "device-001"

# Authentication
api_key: "your-api-key-here"

# Agent behavior
check_in_interval: 60  # seconds between check-ins
log_level: "INFO"  # DEBUG, INFO, WARNING, ERROR

# Vendor-specific settings (optional)
vendor:
  type: "mikrotik"
  connection:
    method: "ssh"  # or "api"
    host: "127.0.0.1"
    username: "admin"
    password: ""
